    <element type ="defaut">
        <couleurTexte nom  = "black" valeurR="0" valeurG="0" valeurB="0" />
        <couleurFond  nom  = "white" valeurR="255" valeurG="255" valeurB="255"/>
        <poids        gras = "false"/>
    </element>

    <element type ="fonction">
        <couleurTexte nom  = "orange" valeurR="150" valeurG="105" valeurB="15" />
        <couleurFond  nom  = "white"  valeurR="255" valeurG="255" valeurB="255"/>
        <poids        gras = "false"/>
    </element>

    <element type ="variable">
        <couleurTexte nom  = "lightgray" valeurR="0"   valeurG="0"   valeurB="0"/>
        <couleurFond  nom  = "white"     valeurR="255" valeurG="255" valeurB="255"/>
        <poids        gras = "false"/>
    </element>

    <element type = "ligneEnCour">
        <couleurTexte nom  = "black" valeurR="0"   valeurG="0"   valeurB="0"/>
        <couleurFond  nom  = "cyan"  valeurR="100" valeurG="245" valeurB="235"/>
        <poids        gras = "false"/>
    </element>

    <element type ="commentaire">
        <couleurTexte nom  ="green"  valeurR="55"  valeurG="150" valeurB="75"/>
        <couleurFond  nom  = "white" valeurR="255" valeurG="255" valeurB="255"/>
        <poids        gras = "false"/>
    </element>

    <element type = "primitive">
        <couleurTexte nom  = "blue"  valeurR="20"  valeurG="175" valeurB="220" />
        <couleurFond  nom  = "white" valeurR="255" valeurG="255" valeurB="255"/>
        <poids        gras = "true"/>
    </element>

    <element type="methode">
        <couleurTexte nom  = "blue"  valeurR="20"  valeurG="175" valeurB="220"/>
        <couleurFond  nom  = "white" valeurR="255" valeurG="255" valeurB="255"/>
        <poids        gras = "false"/>
    </element>
